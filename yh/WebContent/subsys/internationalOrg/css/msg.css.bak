}
#pager {
  text-align:center;
  margin-left:105px;
/*  margin-top:10px;
  margin-left:10px;
  width: auto; */
  height: 30px;
  overflow:hidden;
}
#pager span {
  font: 14px/26px "微软雅黑","宋体";
  font-weight:bold;
  float:left;
  width:auto;
  display:block;
  padding-left: 8px;
  padding-right: 8px;
  text-align:center;
}


#pager .pageup {
  width: 11px;
  height:17px;
