  width: 136px;
  margin-left: 50px!important;
  margin-left: 25px;
}

#container #person #p-right #base #info {
  float: right;
  display: block;
  width: 380px;
  text-align: left;
  padding-left: 30px;
}

#container #person #p-right #base #info h1 {
  font: bold 16px/24px "微软雅黑","宋体";
  color: #7972e4;
  margin-bottom: 10px;
}
#container #person #p-right #base #info table {
  font: 14px/21px "微软雅黑","宋体";
