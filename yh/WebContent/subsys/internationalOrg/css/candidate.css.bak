#container #candidate {
  width: 720px;
  height:420px;
  margin:0 auto;
  text-align: center;
  overflow-x:hidden;
  overflow-y:auto;
/*  padding-left: 10px; */
}
#container #candidate #cd-content {
  padding-left: 0px;
}
#container #candidate .Super {
  padding-left: 10px;
  color: #aaa;
  float: left;
  width: 55px;
  height: auto;
  display: inline;
  font: 80px "Times New Roman", Times, serif;
  line-height: 125px;
  padding-right: 30px;

}
#container #candidate .capital {
  width: 785px;
  min-height: 90px;
  _height: 120px;
  height: auto;
  display: block;
  margin-left:auto;
  margin-right:auto;
  padding: 10px 0;
  background-color:#F3F3F3;
  /* overflow: hidden; */
 
}
#container #candidate .capital:after {
content: ".";
visibility: hidden;
clear: both;
height: 0;
display: block;
}

#container #candidate .capital1 {
  width: 785px;
  min-height: 120px;
  _height: 120px;
  height: auto;
  display: block;
  margin-left:auto;
  margin-right:auto;
  padding: 10px 0;
  background-color:#FAFAFA;
}
#container #candidate .capital1:after {
content: ".";
visibility: hidden;
clear: both;
height: 0;
display: block;
}


#container #candidate ul {
    float: right;
    width: 680px;
    display: inline;
    overflow: hidden;
}

#container #candidate li {
  display: inline;
  width: 100px;
  float: left;
  font: 13px/24px "微软雅黑","宋体";
  color: #252525;
  text-align: left;
  vertical-align: middle;
}
#container #candidate a {
  font: 13px/24px "微软雅黑","宋体";
  color: #252525;
  /* margin-right: 20px;
  width: 60px;
  float: right; */
}
#container  #abc {
  font: 16px/24px "微软雅黑","宋体";
  color: #252525;
  padding-left: 5px;
  width: 800px;
  
  margin-bottom: 10px;
  text-align: center;
  background: url(images/hrline.gif) repeat-x 0 22px;
}
#container  #abc span {
  font: 16px/22px "微软雅黑","宋体";
  padding-left: 10px;
  padding-right: 10px;
}
#container  #abc a {
  color: #e23a38;
  font: 16px/22px "微软雅黑","宋体";
}
#container #person {
  width: 840px;
  height: auto;
  margin:0 auto;
}
#container #person #p-left {
  float: left;
  display: block;
  width: 210px;
  height: auto;
  padding-top: 10px;
}
#container #person #p-left ul {
  width: 200px;
}

#container #person #p-left li {
  padding-left: 10px;
  color: #aaa;
  float: left;
  font: 30px/60px "Times New Roman", Times, serif;
  padding-right: 30px;
  width: 20px;
  height: 60px;
}
#container #person #p-left li.selected {
  padding-left: 0px;
  float: left;
  font: 58px/60px "Times New Roman", Times, serif;
  padding-right: 30px;
}
#container #person #p-left li.selected a {
  font: 58px/60px "Times New Roman", Times, serif;
  color: #326595;
}
#container #person #p-left li a {
  color: #aaa;
  font: 30px/60px "Times New Roman", Times, serif;
}
#container #person #p-left .alphabet {
  margin-left: 0;
  width: 210px;
  height: auto;
  margin-top: 10px;
  margin-bottom: 10px;
  text-align: left;
}
#container #person #p-left .alphabet .hilit a {
  color: #8079e4;
/*  font: 16px/28px "微软雅黑","宋体"; */
}
#container #person #p-left .alphabet ul {
  margin-left: 15px;
  clear: left;
}
#container #person #p-left .alphabet li {
  font: 14px/28px "微软雅黑","宋体";
  color: #000;
  width: 60px;
  height: 21px;
  display: inline;
  /* float: left; */
  zoom: 1;
  padding-left: 0;
  padding-right: 0;
  overflow: hidden;
}
#container #person #p-left .alphabet li.hilit {
  font: 14px/28px "微软雅黑","宋体";
  color: #8079e4;
  width: 60px;
  height: 21px;
  display: inline;
  float: left;
  word-break: keep-all;
}
#container #person #p-left .alphabet a {
  font: 14px/28px "微软雅黑","宋体";
  color: #000;
}
#container #person #p-left #goB{
	clear: left;
	display: block;
}
#container #person #p-right {
  float:right;
  display: block;
  width: 630px!important;
  width: 624px;
  width: 500px
  height: auto;
  padding-bottom: 10px;
  overflow: hidden;
  background: url(images/inset-line.png) repeat-y left top;
}
#container #person #p-right #base {
  width: 610px;
  height: 160px;
  margin-top: 20px;
}
#container #person #p-right #base #photo {
  float: left;
